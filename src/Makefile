OBJS = scanner.cmo

TARFILES = Makefile scanner.mll

sip : $(OBJS)
	ocamlc -o sip $(OBJS)

scanner.ml : scanner.mll
	ocamllex scanner.mll

%.cmo : %.ml
	ocamlc -c $<

%.cmi : %.mli
	ocamlc -c $<

sip.tar.gz : $(TARFILES)
	cd .. && tar czf sip/sip.tar.gz $(TARFILES:%=sip/%)

.PHONY : clean
clean :
	rm -f sip scanner.ml \
	*.cmo *.cmi *.out *.diff

# Generated by ocamldep *.ml *.mli
